    private static void madar_feltoltese(
            String birdName,
            List<String> colors,
            String conservationValue,
            String description,
            List<String> diets,
            List<String> facts,
            List<String> habitats,
            String latinName,
            boolean migratory,
            List<String> shapes,
            String size,
            String wing_span) {

//        Log.d("UPLOAD", birdName +
//                colors +
//                conservationValue +
//                description +
//                diets +
//                facts +
//                habitats +
//                latinName +
//                migratory +
//                shapes +
//                size +
//                wing_span);

        String mainPictureId = "todo";
        List<String> pictureIds = new ArrayList<>();
        pictureIds.add("todo");

        FirebaseFirestore mFirestore = FirebaseFirestore.getInstance();

        Map<String, Object> feltoltendo_madar = new HashMap<>();
        feltoltendo_madar.put("birdName", birdName);
        feltoltendo_madar.put("colors", colors.stream()
                .map(mFirestore::document)
                .collect(Collectors.toList()));
        feltoltendo_madar.put("conservationValue", mFirestore.document(conservationValue));
        feltoltendo_madar.put("description", description);
        feltoltendo_madar.put("diets", diets.stream()
                .map(mFirestore::document)
                .collect(Collectors.toList()));
        feltoltendo_madar.put("facts", facts);
        feltoltendo_madar.put("habitats", habitats.stream()
                .map(mFirestore::document)
                .collect(Collectors.toList()));
        feltoltendo_madar.put("latinName", latinName);
        feltoltendo_madar.put("mainPictureId", mainPictureId);
        feltoltendo_madar.put("migratory", migratory);
        feltoltendo_madar.put("pictureIds", pictureIds);
        feltoltendo_madar.put("shapes", shapes.stream()
                .map(mFirestore::document)
                .collect(Collectors.toList()));
        feltoltendo_madar.put("size", size);
        feltoltendo_madar.put("wingSpan", wing_span);

        Executors.newSingleThreadExecutor().execute(() -> {
            CollectionReference mCollection = mFirestore.collection("birds");
            mCollection.add(feltoltendo_madar)
                    .addOnSuccessListener(documentReference ->
                            System.out.println("Sikeres feltöltés! Dokumentum ID: " + documentReference.getId()))
                    .addOnFailureListener(e ->
                            System.err.println("Hiba történt a feltöltés során: " + e.getMessage()));
        });
    }